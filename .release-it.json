{
  "git": {
    "commitMessage": "chore: release v${version}",
    "push": true,
    "commit": true,
    "tag": false
  },
  "npm": {
    "publish": false
  },
  "github": {
    "release": false
  },
  "hooks": {
    "after:bump": "echo 更新版本成功"
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": {
        "name": "angular"
      },
      "infile": "CHANGELOG.md"
    }
  }
}
